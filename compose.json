{
  "version": "2.1",
  "networks": {
    "default": {
      "external": {
        "name": "ubuntu_default"
      }
    }
  },
  "services": {
    "nginx": {
      "volumes": [
        "/data:/data",
        "/config:/config"
      ],
      "environment": [
        "SERVICE_8888_NAME=docs"
      ],
      "expose": [
        "8888"
      ],
      "ports": [
        "${BLOCK_SSH_PORT}"
      ],
      "cap_add": [
        "NET_ADMIN"
      ],
      "dns_search": "node.dc1.consul service.dc1.consul",
      "image": "${BLOCK_REGISTRY}/block:nginx"
     }
  }
}
