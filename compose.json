{
  "version": "2.1",
  "networks": {
    "default": {
      "external": {
        "name": "ubuntu_default"
      }
    }
  },
  "services": {
    "nginx": {
      "volumes": [
        "/data:/data",
        "/config:/config"
      ],
      "environment": [
        "SERVICE_2222_NAME=ssh",
        "SERVICE_8888_NAME=docs"
      ],
      "expose": [
        "8888"
      ],
      "ports": [
        "${BLOCK_SSH_PORT}"
      ],
      "cap_add": [
        "NET_ADMIN"
      ],
      "networks": {
        "default": {
          "ipv4_address": "${NET_PREFIX}.6"
        }
      },
      "dns": "172.28.128.1",
      "dns_search": "dc1.node.consul",
      "image": "${BLOCK_REGISTRY}/block:nginx",
      "hostname": "nginx"
     }
  }
}
